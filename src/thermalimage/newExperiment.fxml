<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<fx:root alignment="CENTER" prefHeight="300.0" prefWidth="600.0" spacing="20.0" type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/8.0.191" xmlns:fx="http://javafx.com/fxml/1">

   <children>
      <Label fx:id="labelName" alignment="CENTER" minHeight="30.0" prefWidth="300.0" text="Label" />
      <ProgressBar fx:id="progressBar" prefHeight="30.0" prefWidth="400.0" progress="0.0" />
      <Label fx:id="labelInstruction" text="Label" />
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
         <children>
            <Button fx:id="backgroundButton" mnemonicParsing="false" onAction="#backgroundImg" text="Hintergrundbild" />
            <Button fx:id="initialImageinitialImageButton" mnemonicParsing="false" onAction="#initialImage" text="Anfangsbild" />
            <Button fx:id="startExperimentstartExperimentButton" mnemonicParsing="false" onAction="#startExperiment" text="versuch starten" />
            <Button fx:id="abbortButton" mnemonicParsing="false" onAction="#abbort" text="Abbrechen" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets left="10.0" top="10.0" />
   </padding>
</fx:root>
